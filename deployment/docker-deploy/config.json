{
    "config_name": "Puter Docker Deployment",
    "env": "production",
    "http_port": 4100,
    "domain": "puter.localhost",
    "protocol": "http",
    "contact_email": "your-email@example.com",

    "allow_all_host_values": true,
    "allow_nipio_domains": true,
    "disable_ip_validate_event": true,
    "custom_domains_enabled": true,
    "experimental_no_subdomain": true,

    "nginx_mode": true,

    "services": {
        "database": {
            "engine": "sqlite",
            "path": "/var/puter/puter-database.sqlite"
        },
        "dynamo": {
            "path": "/var/puter/puter-ddb"
        },
        "thumbnails": {
            "engine": "purejs"
        },
        "file-cache": {
            "disk_limit": 16384,
            "disk_max_size": 16384,
            "precache_size": 16384,
            "path": "/var/puter/file-cache"
        }
    },

    "cookie_name": "8fbcc83b-6a51-48f6-9a77-82145c76c651",
    "jwt_secret": "5b35719d-f834-49ad-8532-985c787e02b1",
